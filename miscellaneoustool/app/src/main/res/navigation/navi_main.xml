<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navi_main"
    app:startDestination="@id/navi_droplist">

    <fragment
        android:id="@+id/navi_droplist"
        android:name="com.jinproject.twomillustratedbook.ui.screen.navigation.NavigationFragment"
        android:label="droplist">
        <argument
            android:name="start"
            app:argType="string"
            android:defaultValue="dropList"
            />
    </fragment>

    <include app:graph="@navigation/navi_collection"/>

    <fragment
        android:id="@+id/navi_alarm"
        android:name="com.jinproject.twomillustratedbook.ui.screen.navigation.NavigationFragment"
        android:label="alarm">
        <argument
            android:name="start"
            app:argType="string"
            android:defaultValue="alarm"
            />
    </fragment>

    <fragment
        android:id="@+id/navi_symbol"
        android:name="com.jinproject.twomillustratedbook.ui.screen.navigation.NavigationFragment"
        android:label="symbol">
        <argument
            android:name="start"
            app:argType="string"
            android:defaultValue="symbolGraph"
            />
    </fragment>

    <fragment
        android:id="@+id/navi_simulator"
        android:name="com.jinproject.twomillustratedbook.ui.screen.navigation.NavigationFragment"
        android:label="simulator">
        <argument
            android:name="start"
            app:argType="string"
            android:defaultValue="simulator"
            />
    </fragment>
</navigation>